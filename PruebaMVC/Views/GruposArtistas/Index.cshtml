@model IEnumerable<PruebaMVC.Models.VistaGruposArtista>

@{
    ViewData["Title"] = "Index";
}
<p class="pt-3">
    <a class="gordete" asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreArtista)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreGrupo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.NombreArtista)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.NombreGrupo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
