@model PruebaMVC.Models.Lista

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Listas</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Usuario)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Usuario!.Nombre)
        </dd>
    </dl>
</div>
<div class="fondoiconos">
    <a asp-action="Edit" asp-route-id="@Model?.Id"><i class="fa-solid fa-pen"></i></a>
    <a asp-action="Index"><i class="fa-solid fa-rotate-left"></i></a>
</div>
